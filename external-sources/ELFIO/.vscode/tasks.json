{
    "tasks": [
        {
            "type": "shell",
            "label": "ELFIO Test build",
            "command": "make",
            "args": [
                "INCLUDES=-I..",
                "CXXFLAGS='-g -O0'"
            ],
            "options": {
                "cwd": "${workspaceRoot}/tests"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "type": "shell",
            "label": "ELF Dump Build",
            "command": "make",
            "args": [
                "INCLUDES=-I..",
                "CXXFLAGS='-g -O0'"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}